{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "__DEV__": true
  },
  "rules": {
    "indent": "off",
    "no-unused-vars": "off",
    "brace-style": "off",
    "arrow-body-style": [ "error", "always" ],
    "no-console": "off",
    "react/jsx-quotes": 0,
    "jsx-quotes": [2, "prefer-double"],
    "max-len": [2, 200, 4, {"ignoreUrls": true}],
    "no-param-reassign": [2, {"props": false}]
  }
}
